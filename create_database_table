/** In this challenge, I created a database table of 3 books: 1- HITCHHIKERS GUIDE TO THE GALAXY, 2- A HAPPY DEATH 3-DEFICIT MYTH **/

CREATE TABLE books (id INTEGER PRIMARY KEY, name TEXT, rating INTEGER );

INSERT INTO books VALUES (1, "HITCHIKERS GUIDE TO THE GALAXY", 1);
INSERT INTO books VALUES (2, "A HAPPY DEATH", 2);
INSERT INTO books VALUES (3, "THE DEFICIT MYTH",3);
SELECT * FROM books;
